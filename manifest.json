{
  "name": "Jira Server Epic Story Points",
  "description": "View Story Points when viewing an Epic in Jira Server.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://jira.wellspring.com/browse/*"],
      "js": ["scripts/content-script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}